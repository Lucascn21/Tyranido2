<main id="main-user">
	<section id="section-user">
		<h1 class="pb-5 pt-5">Change Password</h1>
		<div class="row g-3 mb-5 align-items-center justify-content-center">
			<div class="col-auto">
				<label for="inputPassword6" class="col-form-label">Old Password</label>
			</div>
			<div class="col-auto">
				<input type="password" id="inputPassword6" class="form-control" aria-describedby="passwordHelpInline" />
			</div>
		</div>
		<div class="row g-3 align-items-center justify-content-center">
			<div class="col-auto">
				<label for="inputPassword6" class="col-form-label">New Password</label>
			</div>
			<div class="col-auto">
				<input type="password" id="inputPassword6" class="form-control" aria-describedby="passwordHelpInline" />
			</div>
			<div id="passwordHelpBlock" class="form-text">
				Your password must be 8-20 characters long, contain letters and numbers, and must not contain spaces, special
				characters, or emoji.
			</div>
		</div>
	</section>
	<section id="section-liked-movies">
		<h1>Favourite Movies</h1>
		<div id="carousel-liked-movies" class="carousel slide" data-bs-ride="carousel">
			<div class="carousel-inner">
				<div class="carousel-item active">
					<img
						class=""
						src="https://m.media-amazon.com/images/M/MV5BOTY4YjI2N2MtYmFlMC00ZjcyLTg3YjEtMDQyM2ZjYzQ5YWFkXkEyXkFqcGdeQXVyMTQxNzMzNDI@._V1_SX300.jpg"
						alt=""
					/>
				</div>
				<div class="carousel-item">
					<img
						class=""
						src="https://m.media-amazon.com/images/M/MV5BOTY4YjI2N2MtYmFlMC00ZjcyLTg3YjEtMDQyM2ZjYzQ5YWFkXkEyXkFqcGdeQXVyMTQxNzMzNDI@._V1_SX300.jpg"
						alt=""
					/>
